    "angular": "~1.2.20",
    "angular-resource": "~1.2.20",
    "angular-ui-router": "~0.2.10",
    "angular-ui-bootstrap-bower": "~0.11.0",
    "bootstrap": "~3.2.0",
    "d3": "~3.4.10",
